{
	"manifest_version": 3,
	"name": "SYHeader",
	"description": "SYHeader ile isteklerinizdeki header bilgilerini değiştirin!",
	"version": "0.3.1",

	"action": {
		"default_icon": "icon.png",
		"default_popup": "popup/index.html"
	},

	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},

	"permissions": [
		"storage",
		"declarativeNetRequest", 
		"declarativeNetRequestFeedback"
	],

	"host_permissions": [
		"<all_urls>"
	]
}